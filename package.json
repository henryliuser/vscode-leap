{
  "name": "szaldivar-leap",
  "displayName": "Leap (with forwards and backwards)",
  "description": "General-purpose motion plugin inspired by leap.nvim forked from the leap extension",
  "version": "0.0.3",
  "publisher": "szaldivar",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "leap",
    "leap.nvim",
    "jump",
    "jumpy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/szaldivar/vscode-leap/tree/new-extension"
  },
  "bugs": {
    "url": "https://github.com/szaldivar/vscode-leap/issues"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "leap.find",
        "title": "Leap Find: Open Widget"
      },
      {
        "command": "leap.find-forwards",
        "title": "Leap Find Forwards: Open Widget"
      },
      {
        "command": "leap.find-backwards",
        "title": "Leap Find Backwards: Open Widget"
      },
      {
        "command": "leap.match-case",
        "title": "Leap Find: Toggle Match Case"
      }
    ],
    "keybindings": [
      {
        "command": "leap.find",
        "key": "ctrl+alt+f"
      },
      {
        "command": "leap.match-case",
        "key": "alt+c"
      }
    ],
    "configuration": [
      {
        "title": "Leap",
        "properties": {
          "leap.whiteSpacesOnlyMatchNewLine": {
            "type": "boolean",
            "default": false,
            "description": "Determines if <space><space> should only match the new Line symbol like in leap.nvim"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "typescript": "^4.9.5"
  }
}
